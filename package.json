{
  "name": "corystack-stealth-scraper",
  "version": "1.0.0",
  "description": "Advanced stealth scraper for security testing and vulnerability assessment",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "corystack-scraper": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "scraper",
    "stealth",
    "security-testing",
    "penetration-testing",
    "bot-detection",
    "anti-bot",
    "web-scraping",
    "playwright",
    "fingerprinting",
    "security",
    "testing"
  ],
  "author": "CoryStack Security Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhishwasher/corystack-bundle.git"
  },
  "bugs": {
    "url": "https://github.com/dhishwasher/corystack-bundle/issues"
  },
  "homepage": "https://github.com/dhishwasher/corystack-bundle#readme",
  "files": [
    "dist",
    "src/types",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "playwright": "^1.40.1",
    "axios": "^1.6.2",
    "bezier-easing": "^2.1.0",
    "tough-cookie": "^4.1.3",
    "cheerio": "^1.0.0-rc.12",
    "redis": "^4.6.12",
    "bullmq": "^5.1.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "table": "^6.8.1",
    "uuid": "^9.0.1",
    "user-agents": "^1.1.0",
    "@faker-js/faker": "^8.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/bezier-easing": "^2.1.3",
    "@types/tough-cookie": "^4.0.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
